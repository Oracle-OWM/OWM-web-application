{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "build:css": "postcss resources/css/tailwind.css -o resources/css/app.css",
        "watch:css": "postcss resources/css/tailwind.css -o resources/css/app.css --watch",
        "env:dev": "cross-env NODE_ENV=development",
        "env:prod": "cross-env NODE_ENV=production",
        "react-scripts:start": "sleep 5 && react-scripts start",
        "react-scripts:build": "react-scripts build",
        "start": "run-p env:dev watch:css react-scripts:start",
        "build": "run-s env:prod build:css react-scripts:build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "@babel/preset-react": "^7.13.13",
        "@fullhuman/postcss-purgecss": "^4.0.3",
        "autoprefixer": "^10.3.4",
        "axios": "^0.21.4",
        "bootstrap": "^4.6.0",
        "cross-env": "^7.0.3",
        "cssnano": "^5.0.8",
        "jquery": "^3.6",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "npm-run-all": "^4.1.5",
        "popper.js": "^1.16.1",
        "postcss": "^8.3.6",
        "postcss-cli": "^8.3.1",
        "purgecss": "^4.0.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "resolve-url-loader": "^4.0.0",
        "sass": "^1.32.11",
        "sass-loader": "^11.0.1",
        "tailwindcss": "^2.2.15"
    },
    "dependencies": {
        "@headlessui/react": "^1.4.1",
        "@heroicons/react": "^1.0.4",
        "@tailwindcss/forms": "^0.3.4",
        "antd": "^4.16.13",
        "firebase": "^9.6.6",
        "gulp": "^4.0.2",
        "js-cookie": "^3.0.1",
        "pusher-js": "^7.5.0",
        "react-qr-code": "^2.0.11",
        "react-responsive": "^9.0.0-beta.4",
        "react-router-dom": "^5.3.0",
        "react-select": "^5.1.0",
        "recharts": "^2.3.2",
        "sweetalert": "^2.1.2",
        "ws": "^8.11.0"
    }
}
